        <endpoint contract="Service.IYoutubeFeed" address="YoutubeFeed" binding="webHttpBinding" behaviorConfiguration="Service.YoutubeFeedBehavior" />
      </service>
    </services>
    <behaviors>
      <endpointBehaviors>
        <behavior name="Service.YoutubeFeedBehavior">
          <webHttp />
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
  <runtime>
